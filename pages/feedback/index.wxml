<Tabs tabs="{{tabs}}" bindtabsItemChange="handleTabsItemChange">
  <view class="fb_main">
    <view class="fb_title">
      问题种类
    </view>
    <view class="fb_tips">
      <text>功能建议</text>
      <text>购买遇到问题</text>
      <text>性能问题</text>
      <text>其他</text>
    </view>
    <view class="fb_content">
      <textarea name="" id="" cols="30" rows="10" placeholder="请描述你的问题">

      </textarea>
      <view class="fd_tools">
        <button>+</button>
        <view class="up_img_item">
          <UpImg></UpImg>
        </view>
        
      </view>
    </view>

    <view class="form_btn_wrap">
      <button type="success_no_circle" size="23" color="#fff">提交</button>
    </view>
  </view>
</Tabs>